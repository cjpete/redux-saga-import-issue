{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "config/webpack.config.dev.js"
            }
        }
    },
    "plugins": [
        "import"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "import/named": 1
    }
}
